<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"

        tools:showIn="@layout/app_bar_accueil"
        tools:context=".Accueil"
        android:gravity="center">

    <Button android:layout_width="@dimen/btn_width"
            android:layout_height="@dimen/btn_heigth"
            android:background="@drawable/btn_form"
            android:text="MTN"
            android:elevation="@dimen/elevation"
            android:id="@+id/btn_MTN"
            android:backgroundTint="@color/colorMTN"
            android:layout_margin="@dimen/margin"

    />

    <Button android:layout_width="@dimen/btn_width"
            android:layout_height="@dimen/btn_heigth"
            android:background="@drawable/btn_form"
            android:text="MOOV"
            android:elevation="@dimen/elevation"
            android:id="@+id/btn_MOOV"
            android:backgroundTint="@color/colorMoov"

    />


</LinearLayout>